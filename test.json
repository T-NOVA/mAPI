{
  "id":"localvNF2",
  "vnfd":{
    "lifecycle_event":{
      "Driver":"SSH",
      "Authentication":"bruno-teste.pem",
      "Authentication_Type":"private key",
      "VNF_Container":"/home/ubuntu/container/",
      "events":[
        {
          "Event":"start",
          "Command":"python /home/ubuntu/local_vnf/start.py",
          "Template File Format":"json",
          "Template File":"{ \"controller\":\"get_attr[vdu1,PublicIp]\"}"
        },
        {
          "Event":"stop",
          "Command":"python /home/ubuntu/local_vnf/stop.py"
        },
        {
          "Event":"scale-out",
          "Command":"python /home/ubuntu/local_vnf/scale-out.py",
          "Template File Format":"json",
          "Template File":"{ \"controller\":\"get_attr[vdu1,PublicIp]\"}"
        }
      ]
    }
  }
}
