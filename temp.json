        "Template File Format":"json",
        "Template File":"{\"controller\":\"cntr_IP\",\"vdu1\":\"vdu1_IP\",\"vdu2\":\"vdu2_IP\"}"
      },
      {
        "Event":"halt",
        "Command":"service vtc shutoff",
        "Template File Format":"json",
        "Template File":"{\"controller\":\"cntr_IP\",\"vdu1\":\"vdu1_IP\",\"vdu2\":\"vdu2_IP\"}"
      }
    ]
  }
